samplea = c(3, 1.5, 1.07)
sampleb = c(8.23, 4.7, 3.7)
wilcox.test(samplea, sampleb)
samplea = c(3, 1.5, 1.07)
samplea = c(1, 1, 1)
sampleb = c(8.23, 4.7, 3.7)
wilcox.test(samplea, sampleb)
